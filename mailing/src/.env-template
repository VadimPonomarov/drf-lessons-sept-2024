APP_CONFIG__CELERY_BROKER=pyamqp://guest:guest@rabbitmq//
APP_CONFIG__CELERY_BACKEND=rpc://
APP_CONFIG__CELERY_INCLUDE=services.mail_services

APP_CONFIG__SENDGRID__API_KEY=
APP_CONFIG__SENDGRID__MY_EMAIL=
