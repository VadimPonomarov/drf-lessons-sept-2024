APP_CONFIG__ENVIRONMENT=dev

APP_CONFIG__DB__ECHO=1
APP_CONFIG__API__PREFIX=/api

APP_CONFIG__AUTH__LOGIN_URL=/login
APP_CONFIG__AUTH__SECRET=MY-SECRET-KEY-JWT
APP_CONFIG__AUTH__ACCESS_TOKEN_LIFETIME=3600

